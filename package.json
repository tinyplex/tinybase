    "@babel/preset-typescript": "^7.17.12",
    "@rollup/plugin-replace": "^4.0.0",
    "@types/asciichart": "^1.5.6",
    "@types/expect-puppeteer": "^4.4.7",
    "@types/http-server": "^0.12.1",
    "@types/jest": "^28.1.1",
    "@types/jest-environment-puppeteer": "^5.0.2",
    "@types/less": "^3.0.3",
    "@types/puppeteer": "^5.4.6",
    "@types/react": "^17.0.41",
